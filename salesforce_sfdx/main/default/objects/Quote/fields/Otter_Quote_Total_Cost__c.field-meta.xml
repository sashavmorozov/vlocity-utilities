<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Otter_Quote_Total_Cost__c</fullName>
    <externalId>false</externalId>
    <formula>if (and(not(isblank(vlocity_cmt__NumberOfContractedMonths__c)), vlocity_cmt__NumberOfContractedMonths__c &lt;&gt; 0), 
  vlocity_cmt__NumberOfContractedMonths__c * vlocity_cmt__EffectiveRecurringCostTotal__c + vlocity_cmt__EffectiveOneTimeCostTotal__c, 
  vlocity_cmt__EffectiveRecurringCostTotal__c + vlocity_cmt__EffectiveOneTimeCostTotal__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Total quote cost for a provider over the period of contract duration. If contract duration is not specified, it is assumed 1 month by default</inlineHelpText>
    <label>Quote Total Cost</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
